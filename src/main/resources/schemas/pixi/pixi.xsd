                            <xs:documentation>Animal weight</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="animal_weight_unit" type="xs:string" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Animal weight unit (g)</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="animal_sacrifice_date" type="xs:date" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Animal sacrifice date</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="animal_sacrifice_time" type="xs:time" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Animal sacrifice time</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="technician" type="xs:string" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Individual responsible for this measurement.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
