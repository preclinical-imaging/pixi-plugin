    <ViewLink alias="PIXI_BIODISTRIBUTIONDATA_PROJECTS">
        <Mapping TableName="PIXI_BIODISTRIBUTIONDATA_PROJECTS">
            <MappingColumn rootElement="pixi:biodistributionData" fieldElement="pixi:biodistributionData.ID"
                           mapsTo="id"/>
        </Mapping>
    </ViewLink>
    <ViewLink alias="BIOD_SAMPLE_TYPE_COUNT_AGG">
        <Mapping TableName="BIOD_SAMPLE_TYPE_COUNT_AGG">
            <MappingColumn rootElement="pixi:biodistributionData" fieldElement="pixi:biodistributionData.ID" mapsTo="pixi_biodistributiondata_id"/>
        </Mapping>
    </ViewLink>
    <SQLView name="biod_sample_type_count_agg" sql="SELECT pixi_biodistributiondata_id, TRIM(', ' FROM xs_a_concat(sample_type || ', ')) AS TYPE_LIST FROM (SELECT DISTINCT pixi_biodistributiondata_id, sample_type FROM pixi_biodSampleUptakeData ORDER BY pixi_biodistributiondata_id, sample_type) _TYPES GROUP BY pixi_biodistributiondata_id"/>
    <SQLView name="PIXI_BIODISTRIBUTIONDATA_PROJECTS"
             sql="SELECT id, '&lt;' || expt.project || '&gt;' || xs_a_concat(',&lt;' || shared.project || '&gt;') AS projects FROM xnat_experimentData expt LEFT JOIN xnat_experimentData_share shared ON expt.id=shared.sharing_share_xnat_experimentda_id LEFT JOIN xdat_meta_element xme ON expt.extension = xme.xdat_meta_element_id WHERE element_name='pixi:biodistributionData' GROUP BY expt.id,expt.project"/>
